{
  "name": "ding-icons",
  "version": "2.0.0",
  "main": "dist/index.cjs.js",
  "sideEffects": [
    "**/*.css",
    "**/*.scss",
    "dist/styles/*.css",
    "dist/fonts/*"
  ],
  "files": [
    "dist",
    "src/styles/icons.css"
  ],
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/es/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/types/index.d.ts",
      "style": "./dist/styles/icons.css"
    }
  },
  "scripts": {
    "dev": "vite build --watch",
    "demo:dev": "cd demo && pnpm install && pnpm dev",
    "demo:build": "cd demo && pnpm install && pnpm build",
    "demo:preview": "cd demo && pnpm preview",
    "build": "rm -rf dist && tsc && vite build",
    "prepack": "pnpm build",
    "test": "vitest",
    "changeset": "pnpm exec changeset",
    "version-packages": "pnpm exec changeset version",
    "release": "pnpm build && pnpm exec changeset publish",
    "postinstall": "pnpm build"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@changesets/cli": "2.28.1",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "changeset": "0.2.6",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.5.2",
    "vite-plugin-static-copy": "2.3.0",
    "vitest": "^0.34.3"
  }
}